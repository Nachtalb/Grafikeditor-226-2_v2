<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<g:VerticalPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="#"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="0" ui:field="labelId"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Project Name"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="txtName" width="267px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Jenkins Project"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox ui:field="cboJenkinsProject" width="277px"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Sonar Project"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox ui:field="cboSonarProject" width="277px"/>
				</g:customCell>
			</g:row>
			<g:row/>
			<g:row>
				<g:customCell>
					<g:Button ui:field="buttonOK" text="OK" />
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="buttonCancel" text="Cancel"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:VerticalPanel>
	
</ui:UiBinder> 